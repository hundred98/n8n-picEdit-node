{
  "name": "n8n-nodes-picEdit",
  "version": "0.1.0",
  "description": "N8N nodes for generating images with text and image overlays",
  "keywords": [
    "n8n",
    "node",
    "nodes",
    "image",
    "generator",
    "overlay",
    "text",
    "canvas",
    "picEdit"
  ],
  "license": "MIT",
  "homepage": "https://github.com/hundred98/n8n-picEdit-node",
  "author": {
    "name": "hundred98",
    "email": "hundred98@163.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hundred98/n8n-picEdit-node.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx tsc && npm run copy-assets",
    "copy-assets": "node -e \"const fs=require('fs'),path=require('path');if(!fs.existsSync('dist'))fs.mkdirSync('dist',{recursive:true});const srcDir='nodes';const destDir='dist';function copyAssets(src,dest){const items=fs.readdirSync(src,{withFileTypes:true});items.forEach(item=>{const srcPath=path.join(src,item.name);const destPath=path.join(dest,item.name);if(item.isDirectory()){if(!fs.existsSync(destPath))fs.mkdirSync(destPath,{recursive:true});copyAssets(srcPath,destPath);}else if(path.extname(item.name)==='.svg'){fs.copyFileSync(srcPath,destPath);}});}copyAssets(srcDir,destDir);\"",
    "dev": "npx tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "npx tslint -p tsconfig.json -c tslint.json",
    "lintfix": "npx tslint -p tsconfig.json -c tslint.json --fix"
  },
  "files": [
    "dist",
    "nodes",
    "python"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/PicEdit.node.js"
    ],
    "icon": "nodes/picEdit/picEdit.svg"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@types/request": "^2.48.13",
    "@types/request-promise-native": "^1.0.21",
    "n8n-core": "^0.128.0",
    "n8n-workflow": "^0.128.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "csv-parser": "^3.0.0",
    "express": "^4.18.1",
    "mime-types": "^2.1.35",
    "sharp": "^0.34.4",
    "uuid": "^9.0.1"
  }
}
