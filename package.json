{
  "name": "n8n-nodes-picedit",
  "version": "0.1.1",
  "description": "A powerful n8n community node for image processing and generation. Create canvases, add text overlays, and composite images with full binary data support.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "node",
    "nodes",
    "image",
    "image-processing",
    "canvas",
    "text-overlay",
    "image-generation",
    "graphics",
    "automation",
    "workflow"
  ],
  "license": "MIT",
  "homepage": "https://github.com/hundred98/n8n-picEdit-node#readme",
  "bugs": {
    "url": "https://github.com/hundred98/n8n-picEdit-node/issues"
  },
  "author": {
    "name": "hundred98",
    "email": "hundred98@163.com",
    "url": "https://github.com/hundred98"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hundred98/n8n-picEdit-node.git"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx tsc && npm run copy-assets",
    "copy-assets": "node -e \"const fs=require('fs'),path=require('path');if(!fs.existsSync('dist'))fs.mkdirSync('dist',{recursive:true});const srcDir='nodes';const destDir='dist';function copyAssets(src,dest){const items=fs.readdirSync(src,{withFileTypes:true});items.forEach(item=>{const srcPath=path.join(src,item.name);const destPath=path.join(dest,item.name);if(item.isDirectory()){if(!fs.existsSync(destPath))fs.mkdirSync(destPath,{recursive:true});copyAssets(srcPath,destPath);}else if(path.extname(item.name)==='.svg'){fs.copyFileSync(srcPath,destPath);}});}copyAssets(srcDir,destDir);\"",
    "dev": "npx tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "npx tslint -p tsconfig.json -c tslint.json",
    "lintfix": "npx tslint -p tsconfig.json -c tslint.json --fix",
    "prepack": "npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 2,
    "nodes": [
      "dist/nodes/picEdit/PicEdit.node.js"
    ]
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "typescript": "~4.9.0"
  },
  "dependencies": {
    "csv-parser": "^3.2.0",
    "sharp": "^0.34.4"
  }
}
